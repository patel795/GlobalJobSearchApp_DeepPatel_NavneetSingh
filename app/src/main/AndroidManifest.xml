        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".Employers_editJobs"/>
        <activity android:name=".Student_Filter_Company" />
        <activity android:name=".Employer_AppliedJobApplications" />
        <activity android:name=".Employer_JobsPosted" />
        <activity android:name=".SignUp" />
        <activity android:name=".StudentView_CompanySearch" />
        <activity android:name=".BaseActivity" />
        <activity android:name=".Employer_View" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>